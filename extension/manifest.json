{
  "name": "A11ySearchScores",
  "action": {},
  "manifest_version": 3,
  "version": "0.1",
  "description": "Displays the A11y score of sites when searching in google.",
  "permissions": [
    "webRequest",
    "activeTab",
    "scripting"
  ],
  "host_permissions":[
    "*://*.google.com/search?q=*"
  ],
  "background": {
    "service_worker": "on_search.js"
  }
}
